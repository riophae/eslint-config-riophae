{
  "name": "eslint-config-riophae",
  "version": "0.13.1",
  "description": "The customized ESLint shareable configs for the development of my open-source projects.",
  "author": {
    "name": "Riophae Lee",
    "email": "riophaelee@gmail.com",
    "url": "https://github.com/riophae"
  },
  "license": "MIT",
  "repository": "riophae/eslint-config-riophae",
  "main": "index.js",
  "files": [
    "index.js",
    "legacy.js",
    "vue.js",
    "rules"
  ],
  "scripts": {
    "lint": "eslint .",
    "find-rules": "eslint-find-rules --unused --deprecated ./index.js && eslint-find-rules --unused --deprecated ./vue.js # legacy preset is intended to be ignored",
    "unit": "ava",
    "test": "npm run lint && npm run unit && npm run find-rules"
  },
  "keywords": [
    "eslintconfig",
    "riophae",
    "code",
    "quality",
    "style",
    "lint",
    "linter",
    "jscs",
    "jshint",
    "jslint",
    "eslint",
    "validate",
    "code style",
    "standard",
    "strict",
    "check",
    "checker",
    "verify",
    "enforce",
    "hint",
    "simple"
  ],
  "devDependencies": {
    "ava": "*",
    "eslint": "^7.1.0",
    "eslint-find-rules": "^3.5.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-unicorn": "^20.0.0",
    "eslint-plugin-vue": "^6.2.2"
  },
  "peerDependencies": {
    "eslint": "^7.1.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-unicorn": "^20.0.0",
    "eslint-plugin-vue": "^6.2.2"
  },
  "dependencies": {
    "eslint-restricted-globals": "^0.2.0"
  },
  "engines": {
    "node": "^10.12.0 || >=12.0.0"
  }
}
